server {
        listen 80;
        listen [::]:80;
        listen 443 ssl;
        listen [::]:443 ssl;
        ssl_certificate     /var/www/cert/cert;
        ssl_certificate_key /var/www/cert/key;

        root /var/www/html/static;

        index index.html;

        server_name static.vampire.rip;

        location / {
            try_files $uri $uri/ =404;
        }
}
